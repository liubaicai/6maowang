<!-- Animate.css -->
<link rel="stylesheet" href="/css/animate.css">
<!-- Icomoon Icon Fonts-->
<link rel="stylesheet" href="/css/icomoon.css">
<!-- Theme style  -->
<link rel="stylesheet" href="/css/photo-style.css">
<!-- Magnific Popup -->
<link rel="stylesheet" href="/css/magnific-popup.css">


<!-- Modernizr JS -->
<script src="/js/modernizr-2.6.2.min.js"></script>
<!-- FOR IE9 below -->
<!--[if lt IE 9]>
<script src="/js/respond.min.js"></script>
<![endif]-->

<div id="fh5co-page">

  <div class="back-to-home btn-circle">
    <a href="/"><span>Back</span><em>Back</em></a>
  </div>

  <div class="container-fluid" id="fh5co-image-grid">
    <div class="grid">
      <div class="grid-sizer"></div>

      <% unless @gallery.photos.nil? %>
          <% @gallery.photos.each do |photo| %>
                <div class="grid-item item">
                  <!--<div class="grid-item item animate-box" data-animate-effect="fadeIn">-->
                <a href="<%= photo.url %>-view2" class="image-popup"
                   title="<%= photo.title %>"
                   exif="<%= photo.exif %>"
                   description="<%= photo.description %>">
                  <div class="img-wrap">
                    <img src="<%= photo.url %>-view8" alt="" class="img-responsive">
                  </div>
                  <div class="text-wrap">
                    <div class="text-inner popup">
                      <div>
                        <h2><%= photo.title %></h2>
                      </div>
                    </div>
                  </div>
                </a>
              </div>
          <% end %>
      <% end %>

    </div>
  </div>

</div>